@options{ :@template"g@import '_colors.scss';
@import 'compass/utilities/general/clearfix';
@import 'compass/css3/box-shadow';
@import 'compass/css3/border-radius';

.general-info {
	/* Compass mixin to fix container not wrapping floated elements if there aren't any non-floated elements */
	@include clearfix;
	clear: both;
	padding: 25px;
	
	p {
		margin: 0 auto;
		width: 80%;
	}
	
	.contact {
		margin-top: 50px;
	}
	
	img {
		/* Compass mixin to ensure box-shadows are consistent between vendors */
		@include box-shadow(0 0px null 25px rgba(0,0,0,.5));
		/* Compass mixin to ensure border-radius is consistent between vendors, including horizontal/vertical radii */
		@include border-radius(15px, 25px);
		width: 80%;
		margin: 0 auto;
		display: block;
	}
	
	.left-col {
		float: left;
		width: 50%;
		
		p {
			margin-top: 10px;
		}
	}
	
	.right-col {
		float: left;
		width: 50%;
	}
